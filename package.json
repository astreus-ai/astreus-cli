{
  "name": "@astreus-ai/astreus-cli",
  "version": "0.5.38",
  "description": "Interactive terminal interface for Astreus AI agents",
  "author": "Berke Kiran",
  "license": "MIT",
  "type": "module",
  "bin": {
    "astreus": "./dist/index.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@astreus-ai/astreus": "^0.5.38",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.6.1",
    "ink": "^5.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.18",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/astreus-ai/astreus-cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "tar": "^7.5.7"
  }
}
